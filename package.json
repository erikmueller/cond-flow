{
  "name": "cond-flow",
  "version": "0.1.2",
  "description": "Elixir style cond for easy javascript control flow",
  "repository": {
    "url": "https://github.com/erikmueller/cond-flow"
  },
  "main": "build/index.js",
  "files": [
    "build/index.js",
    "build/index.d.ts",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "prettier --check '**/*.(json|js|ts)' && xo && ava",
    "prepublish": "npm run build"
  },
  "keywords": [
    "cond",
    "control",
    "elixir"
  ],
  "author": "Erik MuÌˆller",
  "license": "ISC",
  "devDependencies": {
    "xo": "^0.53.1",
    "ava": "^5.1.0",
    "prettier": "2.8.0",
    "typescript": "^4.9.3",
    "ts-node": "^10.9.1",
    "@types/node": "*"
  },
  "type": "module",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "prettier": true
  }
}
